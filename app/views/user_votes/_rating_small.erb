<%= form_for @user_vote, :remote => true do |f| %>
    <% if @user_vote.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@user_vote.errors.count, "error") %> prohibited this movie_rating from being
            saved:</h2>

          <ul>
            <% @user_vote.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <%= f.hidden_field :movie_id, :value => @movie.id %>

    <% (1..10).each do |num| %>
        <%= f.radio_button :rating, "#{num}", :class=>"auto-submit-star {split:2}", :title => "test" %>
    <% end %>
<% end %>




